{
  "target": "master",
  "master": "http://localhost:80",
  "slave": "http://localhost:8000",
  "rescue": {
    "enabled": true,
    "alive": "/alive/",
    "timeout": 120
  },
  "redirection": {
    "enabled": false,
    "type": "permanent",
    "appendUrl": true
  },
  "protocole": "http",
  "appendForwardedProto": true,
  "appendHeaders": {
    "x-forwarded-by": "hotnode"
  },
  "enforceWww": false,
  "listen": [
    8010
  ],
  "ssl": {
    "key": "/etc/ssl/private/key.pem",
    "cert": "/etc/ssl/private/certificate.cer"
  }
}
